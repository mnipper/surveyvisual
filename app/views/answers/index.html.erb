<table class="table table-hover">
  <tr>
    <th>Question</th>
    <th>Response</th>
    <th>DateTime</th>
  </tr>
  <% @answers.each do |answer| %>
    <tr>
      <td><%= answer.question.screen_text %></td>
      <td><%= answer.result_answer %></td>
      <td><%= answer.response_date %></td>
    </tr>
  <% end %>
</table>
